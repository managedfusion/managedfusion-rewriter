<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition name="AzureWebRole" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition" schemaVersion="2012-10.1.8">
  <WebRole name="WebRole" vmsize="Small">
    <Sites>
      <Site name="Web">
        <Bindings>
          <Binding name="HTTP" endpointName="HTTP" />
        </Bindings>
      </Site>
    </Sites>
    <Endpoints>
      <InputEndpoint name="HTTP" protocol="http" port="80" />
    </Endpoints>
  </WebRole>
</ServiceDefinition>